# YouTube Data API v3 Key
# Get your API key from: https://console.cloud.google.com/apis/credentials
YOUTUBE_API_KEY=your_youtube_api_key_here

# YouTube Channel ID for @itzfesi
# You can find this by visiting the channel and looking at the URL or page source
YOUTUBE_CHANNEL_ID=your_channel_id_here

# Telegram Channel Username (without @)
# For public channels, no bot setup required!
# We fetch data directly from the public page: t.me/YourUsername
TELEGRAM_CHANNEL_USERNAME=ItzFesi

# Instagram Username (without @) - RECOMMENDED METHOD
# Works for any public Instagram account
# No API keys or authentication required!
INSTAGRAM_USERNAME=itz.fesi

# Instagram Graph API (Optional - Official but requires Business account)
# Only needed if internal API doesn't work or you need guaranteed reliability
# INSTAGRAM_ACCESS_TOKEN=your_long_lived_access_token_here
# INSTAGRAM_ACCOUNT_ID=your_instagram_business_account_id_here

# Telegram Bot for Notifications
# Create a bot via @BotFather on Telegram and get your token
# Instructions: https://core.telegram.org/bots#6-botfather
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Vercel KV (Upstash Redis) - Auto-populated by Vercel when you add the integration
# Alternatively you can set these manually from Upstash dashboard
KV_REST_API_URL=https://your-redis-instance.upstash.io
KV_REST_API_TOKEN=your_rest_api_token
# Optional: Read-only token if you need separate read access
# KV_REST_API_READ_ONLY_TOKEN=your_read_only_token
# Legacy socket URLs (not used by REST client):
# KV_URL=rediss://...
# REDIS_URL=rediss://...

# Admin Broadcast Token - Generate a long random string for securing the notify endpoint
ADMIN_BROADCAST_TOKEN=generate_a_long_random_string_here

# Optional: Webhook Secret - Add extra security to Telegram webhook endpoint
# Set this and append ?secret=YOUR_SECRET to webhook URL
TELEGRAM_WEBHOOK_SECRET=optional_webhook_secret_here
